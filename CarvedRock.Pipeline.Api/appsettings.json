{
  //"Logging": {
  //  "LogLevel": {
  //    "Default": "Information",
  //    "Microsoft.AspNetCore": "Warning"
  //  }
  //},
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Warning"
      }
    },
    "Filter": [
      {
        //Hp --> Logic: Filters all health check logs which are healthy
        //"https": null, //stackoverflow.com/questions/60621931/filtering-dotnetcore-health-check-logs
        "Name": "ByExcluding",
        "Args": {
          "expression": "StartsWith(RequestPath, '/health/') and StatusCode=200"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
